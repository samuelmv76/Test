package BibliotecaApp;

public class Publicaciones {

	public String titulo;
    public int anio;
    
	public String getTitulo() {
		return titulo;
	}
	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}
	public int getAnio() {
		return anio;
	}
	public void setAnio(int anio) {
		this.anio = anio;
	}
	public Publicaciones(String titulo, int anio) {
		super();
		this.titulo = titulo;
		this.anio = anio;
	}
	public void comprobarPublicaciones() {
		
	}
	
	public Publicaciones() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	@Override
	public String toString() {
		return "Publicaciones [titulo=" + titulo + ", anio=" + anio + "]";
	}
	


}
