package Animal;


public class Main {

	public static void main(String[] args) {
		
		
		Animal caballo = new Animal(5,"caballo"); 
		Animal gallina = new Animal(3,"gallina"); 
		Animal tortuga = new Animal(1,"mbappe"); 
		
		caballo.correr();
		caballo.correr();
		gallina.correr();
		tortuga.correr();
		tortuga.correr();
		tortuga.correr();

		caballo.decirPosicion();
		tortuga.decirPosicion();
		gallina.decirPosicion();
	}

}
