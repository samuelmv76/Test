package BibliotecaApp;

public class Revista {

	private String tituloRevista;
	private int anioRevista;
	
	public Revista() {
		
	}

	public String getTituloRevista() {
		return tituloRevista;
	}

	public void setTituloRevista(String tituloRevista) {
		this.tituloRevista = tituloRevista;
	}

	public int getAnioRevista() {
		return anioRevista;
	}

	public void setAnioRevista(int anioRevista) {
		this.anioRevista = anioRevista;
	}

	public Revista(String tituloRevista, int anioRevista) {
		super();
		this.tituloRevista = tituloRevista;
		this.anioRevista = anioRevista;
	}

	@Override
	public String toString() {
		return "Revista [tituloRevista=" + tituloRevista + ", anioRevista=" + anioRevista + "]";
	}

	
}
