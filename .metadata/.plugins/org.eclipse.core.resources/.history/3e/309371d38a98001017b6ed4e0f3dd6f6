package Animal;


public class Main {

	public static void main(String[] args) {
		
		Animal caballo = new Animal(5,"caballo"); 
		Animal gallina = new Animal(3,"gallina"); 
		Animal tortuga = new Animal(1,"mbappe"); 
		
		Thread hilo1 = new Thread(caballo);
		Thread hilo2 = new Thread(gallina);
		Thread hilo3 = new Thread(tortuga);
		hilo1.start();
		hilo2.start();
		hilo3.start();
		
	}

}
