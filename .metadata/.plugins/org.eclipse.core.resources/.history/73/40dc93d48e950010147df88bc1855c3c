package Animal;


public class Main {

	public static void main(String[] args) {
		
		
		Animal caballo = new Animal(5,"peny"); 
		Animal gallina = new Animal(3,"peny"); 
		Animal tortuga = new Animal(1,"peny"); 
		
		caballo.posicion();
		caballo.correr();
		caballo.posicion();
		
		gallina.posicion();
		gallina.correr();
		gallina.posicion();
		
		tortuga.posicion();
		tortuga.correr();
		tortuga.posicion();
	}

}
